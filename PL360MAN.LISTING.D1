     EXTERNAL PROCEDURE GET(R14) BASE R15; NULL;
     EXTERNAL PROCEDURE PUT(R14) BASE R15; NULL;
     EXTERNAL PROCEDURE KLOSE(R14) BASE R15; NULL;
 
     EXTERNAL PROCEDURE CANCEL(R14) BASE R15; NULL;
 
